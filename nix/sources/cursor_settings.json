{
  "claude-code.claudeProcessWrapper": "/usr/local/bin/claude",
  "claude-code.environmentVariables": [
    {
      "name": "CLAUDE_CODE_SKIP_AUTH_LOGIN",
      "value": "true"
    }
  ],
  "cody.override.serverEndpoint": "http://sourcegraph.corp.stripe.com",
  "cody.override.authToken": "sgp_unused_but_must_be_nonempty",
  "cody.net.proxy.endpoint": "unix://~/.stripeproxy",
  "debug.javascript.automaticallyTunnelRemoteServer": false,
  "editor": {
    "cursorBlinking": "solid",
    "fontFamily": "Operator Mono",
    "fontSize": 16,
    "formatOnSave": true
  },
  "explorer": {
    "confirmDelete": false,
    "confirmDragAndDrop": false
  },
  "files": {
    "associations": {
      "ghostty_config": "ini"
    },
    "autoSave": "afterDelay",
    "autoSaveWhenNoErrors": true
  },
  "github.copilot.enable": {
    "*": false,
    "apex": true,
    "erb": true,
    "go": true,
    "java": true,
    "javascript": true,
    "python": true,
    "ruby": true,
    "rust": true,
    "scala": true,
    "shellscript": true,
    "starlark": true,
    "terraform": true,
    "typescript": true,
    "typescriptreact": true
  },
  "gitlens.remotes": [
    {
      "domain": "git.corp.stripe.com",
      "type": "GitHub"
    }
  ],
  "gitlens.ai.experimental.generateCommitMessage.enabled": false,
  "gitlens.ai.enabled": false,
  "remote.SSH.configFile": "~/.ssh/devboxes",
  "remote.SSH.connectTimeout": 60,
  "remote.SSH.enableAgentForwarding": false,
  "remote.SSH.enableDynamicForwarding": false,
  "remote.SSH.enableRemoteCommand": true,
  "remote.SSH.path": "sc-ssh-wrapper",
  "remote.SSH.useLocalServer": true,
  "remote.autoForwardPorts": false,
  "remote.autoForwardPortsSource": "output",
  "remote.forwardOnOpen": false,
  "remote.otherPortsAttributes": {
    "onAutoForward": "ignore"
  },
  "remote.restoreForwardedPorts": false,
  "ruby.rubocop.hideDisableSuggestions": true,
  "search.followSymlinks": false,
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "typescript.tsdk": "~/stripe/pay-server/frontend/js-scripts/node_modules/typescript/lib",
  "typescript.native-preview.tsdk": "~/stripe/pay-server/frontend/js-scripts/scripts",
  "update.mode": "none",
  "window.commandCenter": true,
  "window.title": "${rootPath}${separator}${activeEditorMedium}",
  "workbench": {
    "colorTheme": "Tokyo Night Storm",
    "editor": {
      "closeOnFileDelete": true,
      "highlightModifiedTabs": true
    }
  }
}